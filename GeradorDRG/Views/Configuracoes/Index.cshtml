                <th>
                    @Html.DisplayNameFor(model => model.WebServiceUsuario)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.WebServiceSenha)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Banco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sistema)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BancoURL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BancoUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BancoSenha)
