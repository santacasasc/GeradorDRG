@model IEnumerable<GeradorDRG.Models.Configuracao>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.BancoURL)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BancoUsuario)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BancoSenha)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BancoSID)
