            <td>
                @Html.DisplayFor(modelItem => item.NomeDRG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UtilizaWebService)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WebServiceUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WebServiceSenha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Banco.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sistema.Nome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
