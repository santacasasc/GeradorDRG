@model GeradorDRG.Models.PacienteTeste

<div class="form-group">
    <input type="hidden" name="Pacientes.Index" value="@ViewBag.Index" />
    <label class="control-label" for="Pacientes[@ViewBag.Index].Nome">Nome do paciente</label>
    <input name="Pacientes[@ViewBag.Index].NomePaciente" class="form-control" id="Pacientes[@ViewBag.Index].NomePaciente" aria-required="true" required="required" type="text" value="@Model.NomePaciente">
    <span class="text-danger field-validation-valid" data-valmsg-replace="true" data-valmsg-for="Pacientes[@ViewBag.Index].NomePaciente"></span>

    <label class="control-label" for="Pacientes[@ViewBag.Index].Nome">Código do paciente</label>
    <input name="Pacientes[@ViewBag.Index].CodPaciente" class="form-control" id="Pacientes[@ViewBag.Index].CodPaciente" aria-required="true" required="required" type="text" value="@Model.CodPaciente">
    <span class="text-danger field-validation-valid" data-valmsg-replace="true" data-valmsg-for="Pacientes[@ViewBag.Index].CodPaciente"></span>
    <a href="#" class="btn btn-danger remove_field">Remove</a>


</div>